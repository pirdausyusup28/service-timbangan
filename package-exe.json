{
  "name": "servicetimbangan",
  "version": "1.0.0",
  "description": "Service Timbangan untuk 4 PC",
  "main": "timbangan-service.js",
  "scripts": {
    "start": "node timbangan-service.js",
    "build": "pkg timbangan-service.js --targets node18-win-x64 --output timbangan-service.exe",
    "build-all": "pkg timbangan-service.js --targets node18-win-x64,node18-linux-x64 --out-path dist"
  },
  "bin": "timbangan-service.js",
  "pkg": {
    "assets": [
      "data/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "basic-ftp": "^5.0.4",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "serialport": "^13.0.0",
    "ws": "^8.18.3"
  }
}